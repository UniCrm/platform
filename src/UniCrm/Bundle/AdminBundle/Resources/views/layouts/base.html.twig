    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-rowgroup/dataTables.rowGroup.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-scroller/dataTables.scroller.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-responsive/dataTables.responsive.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-responsive-bs4/responsive.bootstrap4.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-buttons/dataTables.buttons.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-buttons/buttons.html5.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-buttons/buttons.flash.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-buttons/buttons.print.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-buttons/buttons.colVis.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-buttons-bs4/buttons.bootstrap4.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/moment/moment.min.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/select2/select2.min.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/bootstrap-datepicker/bootstrap-datepicker.min.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/admin/plugins/jquery-validation/jquery.validate.min.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/admin/plugins/jquery-validation/form.min.js') }}"></script>
{% endblock %}

{% block js %}
    <!-- Scripts -->
    <script src="{{ asset('bundles/unicrmapp/assets/global/js/Component.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/js/Plugin.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/js/Base.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/js/Config.js') }}"></script>

    <script src="{{ asset('bundles/unicrmapp/assets/mmenu/assets/js/Section/Menubar.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/mmenu/assets/js/Section/Sidebar.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/mmenu/assets/js/Section/PageAside.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/mmenu/assets/js/Section/GridMenu.js') }}"></script>

    <!-- Config -->
    <script src="{{ asset('bundles/unicrmapp/assets/global/js/config/colors.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/mmenu/assets/js/config/tour.js') }}"></script>
    <script>Config.set('assets', '../../assets');</script>

    <!-- Page -->
    <script src="{{ asset('bundles/unicrmapp/assets/mmenu/assets/js/Site.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/js/Plugin/asscrollable.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/js/Plugin/slidepanel.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/js/Plugin/switchery.js') }}"></script>


    <!-- App js -->
    <script src="{{ asset('bundles/unicrmapp/assets/admin/js/app.js') }}"></script>

    <script>
        (function(document, window, $){
            'use strict';

            var Site = window.Site;
            $(document).ready(function(){
                Site.run();
            });

        })(document, window, jQuery);


        /**
         * APP HELPER DECLARATION
         * APP HELPER IS TO BE ADDED ON BASE BUNDLE BACAUSE
         * IT SERVES TO CONFIG THE PLUGINS GLOBALLY
         */

        AppHelper = {};
        AppHelper.baseUrl = "";
        AppHelper.assetsDirectory = "";

        AppHelper.settings = {};
        AppHelper.settings.firstDayOfWeek = 1;
        AppHelper.settings.displayLength = 25;
        AppHelper.settings.timeFormat = "";


        AppLanugage = {};
        AppLanugage.locale =  "EN";

        AppLanugage.days = ["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"];
        AppLanugage.daysShort = ["Sun","Mon","Tue","Wed","Thu","Fri","Sat"];
        AppLanugage.daysMin = ["Su","Mo","Tu","We","Th","Fr","Sa"];

        AppLanugage.months = ["January","February","March","April","May","June","July","August","September","October","November","December"];
        AppLanugage.monthsShort = ["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"];

        AppLanugage.today = "Today";
        AppLanugage.yesterday = "Yesterday";
        AppLanugage.tomorrow = "Tomorrow";

        AppLanugage.search = "Search";
        AppLanugage.noRecordFound = "No Record Found";
        AppLanugage.print ="Print";
        AppLanugage.excel = "Exel";
        AppLanugage.printButtonTooltip = "";

        AppLanugage.fileUploadInstruction = "";
        AppLanugage.fileNameTooLong = "";

        AppLanugage.custom = "";
        AppLanugage.clear = "";
    </script>
{% endblock %}

{% block custom_js %}
    {% if custom_js is defined %}
        {% if custom_js is iterable %}
            {% for  js in custom_js %}
                <script src="{{ asset(js) }}"></script>
            {% endfor %}
        {% endif %}
    {% endif %}
{% endblock %}
</body>
</html>
