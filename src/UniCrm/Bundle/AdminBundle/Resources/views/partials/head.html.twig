<head>
    {% block meta %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
        <meta name="description" content="bootstrap admin template">
        <meta name="author" content="">
    {% endblock %}

    <title> {% block title %} Default Title {% endblock %}</title>

    {% block icons %}
        <link rel="apple-touch-icon" href="{{ asset('bundles/unicrmapp/assets/mmenu/assets/images/apple-touch-icon.png') }}">
        <link rel="shortcut icon" href="{{ asset('bundles/unicrmapp/assets/mmenu/assets/images/favicon.ico') }}">
    {% endblock %}

    {% block stylesheets %}
        <!-- Stylesheets -->
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/css/bootstrap-extend.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/mmenu/assets/css/site.min.css') }}">
    {% endblock %}

    {% block plugin_stylesheets %}
        <!-- Plugins -->
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/vendor/animsition/animsition.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/vendor/asscrollable/asScrollable.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/vendor/switchery/switchery.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/vendor/intro-js/introjs.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/vendor/slidepanel/slidePanel.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/vendor/jquery-mmenu/jquery-mmenu.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/vendor/flag-icon-css/flag-icon.css') }}">
    {% endblock %}

    {% block fonts %}
        <!-- Fonts -->
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/fonts/web-icons/web-icons.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/unicrmapp/assets/global/fonts/brand-icons/brand-icons.min.css') }}">
        <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Roboto:300,400,500,300italic'>

    {% endblock %}

    <!--[if lt IE 9]>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/html5shiv/html5shiv.min.js') }}"></script>
    <![endif]-->

    <!--[if lt IE 10]>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/media-match/media.match.min.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/respond/respond.min.js') }}"></script>
    <![endif]-->

    <!-- Scripts -->
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/breakpoints/breakpoints.js') }}"></script>
    <script>
        Breakpoints();
    </script>
</head>