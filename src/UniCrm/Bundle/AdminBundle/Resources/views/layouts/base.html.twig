    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/jquery-mmenu/jquery.mmenu.min.all.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/switchery/switchery.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/intro-js/intro.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/screenfull/screenfull.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/slidepanel/jquery-slidePanel.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net/jquery.dataTables.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-bs4/dataTables.bootstrap4.js') }}"></script>
    <script src="{{ asset('bundles/unicrmapp/assets/global/vendor/datatables.net-fixedheader/dataTables.fixedHeader.js') }}"></script>
